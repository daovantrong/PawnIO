LIBRARY ntoskrnl.exe
EXPORTS
    ; Memory management
    ExAllocatePoolWithTag
    ExFreePoolWithTag
    
    ; Resource management
    ExInitializeResourceLite
    ExDeleteResourceLite
    ExAcquireResourceExclusiveLite
    ExAcquireResourceSharedLite
    ExReleaseResourceLite
    
    ; Synchronization
    KeInitializeSpinLock
    KeAcquireSpinLock
    KeReleaseSpinLock
    KeInitializeMutex
    KeWaitForSingleObject
    KeReleaseMutex
    
    ; I/O Manager
    IoCreateDevice
    IoDeleteDevice
    IoCreateSymbolicLink
    IoDeleteSymbolicLink
    
    ; String functions
    RtlInitUnicodeString
    RtlCompareUnicodeString
    RtlCopyUnicodeString
    
    ; Debug
    DbgPrint
    DbgPrintEx
    
    ; Bug check
    KeBugCheckEx
    
    ; Interlocked operations
    InterlockedIncrement
    InterlockedDecrement
    InterlockedExchange
    InterlockedCompareExchange
    
    ; Memory operations
    RtlCopyMemory
    RtlZeroMemory
    RtlMoveMemory
    
    ; Process/Thread
    PsGetCurrentProcess
    PsGetCurrentThread
    KeGetCurrentIrql
    
    ; Time
    KeQuerySystemTime
    KeQueryTickCount
    
    ; Registry
    ZwOpenKey
    ZwQueryValueKey
    ZwSetValueKey
    ZwClose
    
    ; Physical memory
    MmMapIoSpace
    MmUnmapIoSpace
    MmGetPhysicalAddress
    
    ; MDL
    MmBuildMdlForNonPagedPool
    IoAllocateMdl
    IoFreeMdl
    
    ; Processor
    KeSetSystemAffinityThread
    KeRevertToUserAffinityThread
    KeGetCurrentProcessorNumber
    
    ; Crypto (BCrypt)
    BCryptOpenAlgorithmProvider
    BCryptCloseAlgorithmProvider
    BCryptGenerateSymmetricKey
    BCryptEncrypt
    BCryptDecrypt
    BCryptDestroyKey
    
    ; Exception handling
    RtlCaptureContext
    RtlLookupFunctionEntry
    RtlVirtualUnwind
